{
	"id": "jenkins",
	"instances": 1,
	"mem": 1500,
	"env": {
		"JAVA_OPTS": "-Xmx300m"
   },
   "uris": [
      "https://s3.amazonaws.com/downloads.mesosphere.io/jenkins/0.6.0/jenkins-on-mesos.tgz"
   ],
   "cmd": "cd jenkins-on-mesos && java -DJENKINS_HOME=$(pwd) -jar jenkins.war --httpPort=$PORT0",
   "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 30,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}